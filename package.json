{
    "name": "gitbook-styleguide",
    "version": "1.0.4",
    "description": "Styleguide for GitBook websites (less, branding, logos)",
    "dependencies": {
        "octicons": "2.2.0",
        "gitbook-logos": "git+https://github.com/GitbookIO/logos.git#2.0.0"
    },
    "devDependencies": {
        "gulp": "^3.8.11",
        "gulp-rename": "^1.2.2",
        "gulp-less": "3.0.2",
        "gulp-run-sequence": "0.3.2",
        "gulp-jekyll": "0.0.0",
        "del": "1.1.1",
        "gulp-connect": "2.2.0",
        "gulp-gh-pages": "0.5.1"
    },
    "scripts": {
        "postinstall": "cp -R ./node_modules/octicons/octicons ./assets/fonts/ && cp -R ./node_modules/gitbook-logos/output ./assets/images/logo"
    }
}