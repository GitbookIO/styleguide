{
  "name": "gitbook-styleguide",
  "version": "6.2.1",
  "main": "./index.js",
  "author": "GitBook <contact@gitbook.com>",
  "homepage": "http://styleguide.gitbook.com/",
  "description": "Styleguide for GitBook websites (CSS, Less, Logos)",
  "bugs": "https://github.com/GitbookIO/styleguide/issues",
  "license": "Apache-2.0",
  "dependencies": {
    "classnames": "^2.2.5",
    "gitbook-markdown-css": "1.0.1",
    "moment": "^2.14.1",
    "moment-duration-format": "^1.3.0",
    "octicons": "3.4.1",
    "react": "^15.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "cssmin": "^0.4.3",
    "eslint": "^2.11.1",
    "eslint-config-gitbook": "1.2.0",
    "gh-pages": "^0.11.0",
    "gitbook-logos": "git+https://github.com/GitbookIO/logos.git#2.0.2",
    "http-server": "^0.9.0",
    "less": "^2.7.1",
    "next": "^1.1.0",
    "roboto-fontface": "^0.6.0"
  },
  "scripts": {
    "lint": "eslint ./src",
    "build-js": "babel src --source-maps inline --out-dir lib",
    "build-css": "lessc ./less/main.less ./static/css/gitbook.css --include-path='node_modules'",
    "build-css-min": "cssmin ./static/css/gitbook.css ./static/css/gitbook.min.css",
    "build-static": "cp -R ./node_modules/octicons/octicons ./static/fonts/ && rm -rf ./static/images/logo && cp -R ./node_modules/gitbook-logos/output ./static/images/logo",
    "build": "npm run build-css; npm run build-js; npm run build-static",
    "docs": "npm run build-css; next",
    "docs-build": "next build",
    "test": "npm run lint",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GitbookIO/styleguide.git"
  }
}
